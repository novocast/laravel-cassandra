{
    "name": "novocast/cassandra",
    "description": "Cassandra Eloquent model and Query builder for Laravel",
    "keywords": ["laravel","eloquent","cassandra","database","model"],
    "homepage": "https://github.com/novocast/laravel-cassandra",
    "authors": [
        {
            "name": "Anthony Totton",
            "homepage": "https://byant.co.uk"
        }
    ],
    "license" : "MIT",
    "require": {
        "illuminate/support": "^5.1",
        "illuminate/container": "^5.1",
        "illuminate/database": "^5.1",
        "illuminate/events": "^5.1",
        "datastax/cassandra": "^1.0.0"

    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "orchestra/testbench": "^3.1",
        "mockery/mockery": "^0.9",
        "satooshi/php-coveralls": "^0.6"
    },
    "autoload": {
        "psr-0": {
            "novocast\\Cassandra": "src/",
            "novocast\\Eloquent": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/models",
            "tests/seeds"
        ]
    }
}
